{
  "name": "notion-client-for-browser",
  "version": "6.12.12",
  "type": "module",
  "description": "Robust TypeScript client for the unofficial Notion API. Browsified. But because the Notion server doesn't allow CORS, You can only use it for an extension",
  "repository": "MagnusLim/react-notion-x",
  "author": "Magnus Lim",
  "license": "MIT",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "sideEffects": false,
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch --silent --onSuccess 'echo build successful'",
    "test": "ava"
  },
  "dependencies": {
    "got": "^11.8.1",
    "ky": "^0.30.0",
    "ky-universal": "^0.10.1",
    "notion-types": "^6.12.6",
    "notion-utils": "^6.12.9",
    "p-map-browser": "^1.2.0"
  },
  "ava": {
    "snapshotDir": ".snapshots",
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--no-warnings",
      "--experimental-specifier-resolution=node"
    ]
  }
}
